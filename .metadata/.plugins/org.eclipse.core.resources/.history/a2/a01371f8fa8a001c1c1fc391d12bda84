package com.ey.training.basics;

public class SampleProgram {

		int a = 10;
		int b = 20;
	
		public void addTwoValues() {
			int c=a+b;
			System.out.println("addition: "+c);
		}
	
		public void subtracTwoValues() {
			int c=a-b;
			System.out.println("subtraction: "+c);
		}
		  public void main(String[] args) {
			  addTwoValues();
			  subtractTwoValues();
	   }
}
